{
  "name": "thuan-blog-backend",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "db:migrate": "npx sequelize-cli db:migrate",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write --loglevel warn",
    "fix:tslint": "tslint --fix --project .",
    "codegen": "graphql-codegen --config codegen.yml",
    "predev": "yarn codegen",
    "dev": "nodemon src/index.ts",
    "migrate:start": "yarn db:migrate && ts-node src/index.ts",
    "start": "ts-node src/index.ts",
    "test:lint": "tslint --project . && prettier \"src/**/*.ts\" --list-different",
    "test:types": "tsc -p . --noEmit",
    "test:unit": "jest --watch --verbose",
    "test:unit:ci": "jest --ci"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/merge": "^8.3.3",
    "@graphql-tools/utils": "^8.10.0",
    "apollo-datasource": "^0.6.3",
    "apollo-server": "^3.10.1",
    "apollo-server-express": "^3.10.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^14.5.4",
    "jest": "^29.3.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@graphql-codegen/add": "^1.7.0",
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^1.7.0",
    "@graphql-codegen/typescript-resolvers": "^1.7.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^12.7.3",
    "husky": "^3.0.5",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  }
}
